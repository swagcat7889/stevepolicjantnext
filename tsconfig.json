{
  "compilerOptions": {
    "incremental": true,
    "target": "ES2020",
    "module": "commonjs",
    "lib": [
      "ES5",
      "DOM",
      "ScriptHost",
      "ES2018"
    ],
    "allowJs": false,
    "checkJs": false,
    "jsx": "preserve",
    "declaration": true,
    "declarationMap": false,
    "sourceMap": false,
    "composite": true,
    "removeComments": true,
    "noEmit": false,
    "importHelpers": false,
    "downlevelIteration": false,
    "isolatedModules": false,
    "strict": false,
    "noImplicitAny": false,
    "strictNullChecks": true,
  }
}